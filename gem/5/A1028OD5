#include "bbs.h"

typedef struct {....} OLD;
typedef struct {....} NEW;

int main()
{
  int num;
  OLD old;
  NEW new;

  num = 0;
  while (!rec_get("old-file", &old, sizeof(OLD), num))
  {
    new.xxx = old.xxx;              /* å¦‚æœ xxx æ¬„ä½æ˜¯ int */
    strcpy(new.yyy, old.yyy);       /* å¦‚æœ yyy æ¬„ä½æ˜¯ char[] */
    rec_add("new-file", &new, sizeof(NEW));
    num++;
  }
}

