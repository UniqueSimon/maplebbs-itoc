int
Class2()
{
  int chn, min_chn;
  short *chx;
  char *img;
  const char *name = "ABCDEFG/";  [1;44m // åˆ†é¡ç¾¤çµ„åç¨±è‡ªå®š [m

  [1;44m // ä¹Ÿå¯ä»¥æŠŠä¸‹é¢é€™è¡Œæ‹¿æ‰ï¼Œä½†è‹¥è©²åˆ†é¡è¢« zap å…‰å…‰å°±æœƒé€²ä¸å» [m
  class_flag |= BFO_YANK;   /* æ‹¿æ‰ yankï¼Œé¿å…æŠŠæ‰€æœ‰æ¿éƒ½ zap å¾Œé€²ä¸å» */

  min_chn = bshm->min_chn;
  img = class_img;

  for (chn = CH_END - 2; chn >= min_chn; chn--)
  {
    chx = (short *) img + (CH_END - chn);
    if (!strncmp(img + *chx, name, strlen(name)))
    {
      if (XoClass(chn))
        return 0;
    }
  }

  vmsg("ç„¡æ­¤åˆ†é¡ç¾¤çµ„");
  return XEASY;
}

