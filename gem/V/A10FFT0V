#include "bbs.h"

#ifdef HAVE_GAME

#define FN_STOCK      "stock"       /* å€‹äººçš„è‚¡å¸‚æª”æ¡ˆï¼å„è‚¡æ“æœ‰å¼µæ•¸ */

static char stockname[MAXSTOCK][9] =
{
  "ä¸Š é–¤ å±‹", "æ–° å¤© åœ°", "é æ±ç™¾è²¨", "çµ±è¯å®¢é‹",
  "åœ‹è³“å½±åŸ", "ç”Ÿæ´»å·¥åŠ", "å¯¶ é›… é¤¨", "æˆå¤§é†«å­¸"
};

static int own[MAXSTOCK];


/*-------------------------------------------------------*/
/* è¢å¹•æ§åˆ¶                                              */
/*-------------------------------------------------------*/


static void
show_stock_data(sto)
  STOCK *sto;
{
  int i, change, total, allchange, allordernum, allmoney;

  move(2, 0);
  outs("   \033[1;42m   ç·¨è™Ÿ  \033[36må•†åº—åç¨±    \033[33måƒ¹  æ ¼"
    "\033[37m(\033[31mæ¼²    \033[32mè·Œ\033[37m)  ç›®å‰æ•¸é‡ ä»Šæ—¥æˆäº¤é‡"
    "   æ“æœ‰æ•¸é‡   \033[m\n");
  outs("   \033[46mâ—‡â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€"
    "â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â—‡\033[m\n");

  total = change = ordernum = allmoney = 0;

  for (i = 0; i < MAXSTOCK; i++)
  {
    change = sto->change[i];

    total += sto->price[i];
    allchange += change;
    allordernum += sto->ordernum[i];
    allmoney += own[i] * sto->price[i];

    prints("\033[1m%8d.\033[36m%11s \033[3%dm%8d(%s %8d)"
      "\033[37m%10d%11d%11d\033[m\n",
      i + 1, stockname[i],
      change ? (change > 0 ? 1 : 2) : 7,
      sto->price[i],
      change ? (change > 0 ? "â–³" : "â–½") : "ï¼",
      abs(change),
      sto->stillnum[i], sto->ordernum[i], own[i]);
  }

  outs("   \033[46mâ—‡â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€"
    "â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â—‡\033[m\n");

  prints("\033[1;37m       åŠ æ¬Šè‚¡åƒ¹ç¸½æŒ‡æ•¸ï¼š\033[3%dm%d  %2s %d"
    "\033[37m      ç¸½æˆäº¤é‡ï¼š%d\033[m\n\n",
    allchange ? (allchange > 0 ? 1 : 2) : 7, total,
    allchange ? (allchange > 0 ? "â–²" : "â–¼") : "ï¼", allchange, allordernum);

  prints("   \033[1;32;45mæ‚¨çš„è‚¡åƒ¹ç¸½å€¼ï¼š%d éŠ€å¹£\033[m\n\n", allmoney);
  prints("   \033[1;33;45mæ‚¨çš„è³‡æœ¬é‡‘é¡ï¼š%d éŠ€å¹£\033[m", cuser.money);
}


/*-------------------------------------------------------*/
/* è‚¡å¸‚æŒ‡ä»¤                                              */
/*-------------------------------------------------------*/


static void
stock_query(sto)
  STOCK *sto;
{
  int i;

  clear();
  outs("\033[1;33mæ«ƒå°å°å§çµ¦æ‚¨ä¸€å¼µæ¸…å–®ï¼š\033[\n");
  outs("\033[1;32mè‚¡å¸‚ï¼š\033[32mæŒè‚¡ä¸€è¦½è¡¨\033[m\n\n");
  for (i = 0; i < MAXSTOCK; i++)
  {
    prints("\033[1;3%dm %d.æ‚¨æ“æœ‰ %s çš„è‚¡ä»½å…± %8d å¼µï¼Œè‚¡åƒ¹ [%d]\033[m\n",
      i & 7, i, stockname[i], own[i], sto->price[i]);
  }
  vmsg(NULL);
}


static void
stock_sell(sto)
  STOCK *sto;
{
  int i, num, money;
  char buf[80], ans[8];

  sprintf(buf, "è¦è³£å“ªå®¶(1-%d)ï¼Ÿ", MAXSTOCK);
  vget(b_lines, 0, buf, ans, 2, DOECHO);
  i = atoi(ans) - 1;
  if (i < 0 || i >= MAXSTOCK)
    return;

  if (!own[i])
  {
    vmsg("å·²ç„¡è‚¡ä»½å¯è³£");
    return;
  }

  sprintf(buf, "è¦è³£å¹¾å¼µ(1-%d)ï¼Ÿ", own[i]);
  vget(b_lines, 0, buf, ans, 5, DOECHO);
  num = atoi(ans);
  if (num <= 0 || num > own[i])
    return;

  /* é‡æ–°å–å¾—ç›®å‰è‚¡å¸‚ä¸¦æ›´æ–° */
  rec_get(FN_RUN_STOCK, sto, sizeof(STOCK), 0);
  sto->stillnum[i] += num;
  sto->ordernum[i] += num;
  rec_put(FN_RUN_STOCK, sto, sizeof(STOCK), 0, NULL);

  /* æ›´æ–°æŒè‚¡ */
  own[i] -= num;
  usr_fpath(buf, cuser.userid, FN_STOCK);
  rec_put(buf, own, sizeof(own), 0, NULL);

  money = sto->price[i] * num * 9 / 10;    /* æ‰‹çºŒè²» */
  addmoney(money);

  sprintf(buf, "è³£å‡º %s %d å¼µï¼Œæ”¶å…¥ %d éŠ€å¹£ï¼", stockname[i], num, money);
  vmsg(buf);
}


static void
stock_buy(sto)
  STOCK *sto;
{
  int i, num, money;
  char buf[80], ans[8];

  sprintf(buf, "è¦è²·å“ªå®¶(1-%d)ï¼Ÿ", MAXSTOCK);
  vget(b_lines, 0, buf, ans, 2, DOECHO);
  i = atoi(ans) - 1;
  if (i < 0 || i >= MAXSTOCK)
    return;

  if (sto->stillnum[i] <= 0)
  {
    vmsg("å·²ç„¡è‚¡ä»½å¯è²·");
    return;
  }

  money = cuser.money / sto->price[i];
  sprintf(buf, "è¦è²·å¹¾å¼µ(1-%d)ï¼Ÿ", money);
  vget(b_lines, 0, buf, ans, 5, DOECHO);
  num = atoi(ans);
  if (num <= 0 || num > money)
    return;

  /* é‡æ–°å–å¾—ç›®å‰è‚¡å¸‚ */
  rec_get(FN_RUN_STOCK, sto, sizeof(STOCK), 0);

  if (sto->stillnum[i] < num)
  {
    vmsg("è‚¡ä»½å‰›è¢«æ¶èµ°äº†ï¼Œä¸‹æ¬¡è«‹æ—©ï¼");
    return;
  }

  money = sto->price[i] * num;
  if (cuser.money < money)
  {
    vmsg("è‚¡åƒ¹å·²è®Šå‹•ï¼Œæ‚¨ä¸å¤ é‡‘éŒ¢è³¼è²·");
    return;
  }

  /* æ›´æ–°è‚¡å¸‚ */
  sto->stillnum[i] -= num;
  sto->ordernum[i] += num;
  rec_put(FN_RUN_STOCK, sto, sizeof(STOCK), 0, NULL);

  /* æ›´æ–°æŒè‚¡ */
  own[i] += num;
  usr_fpath(buf, cuser.userid, FN_STOCK);
  rec_put(buf, own, sizeof(own), 0, NULL);

  cuser.money -= money;

  sprintf(buf, "è²·é€² %s %d å¼µï¼ŒèŠ±è²» %d éŠ€å¹£ï¼", stockname[i], num, money);
  vmsg(buf);
}


/*-------------------------------------------------------*/
/* ä¸»ç¨‹å¼                                                */
/*-------------------------------------------------------*/


int
main_stock()
{
  int i;
  char fpath[64];
  STOCK sto;

  if (cutmp->status & STATUS_COINLOCK)
  {
    vmsg(MSG_COINLOCK);
    return XEASY;
  }

  /* æˆ‘çš„æŒè‚¡ */
  usr_fpath(fpath, cuser.userid, FN_STOCK);
  if (rec_get(fpath, own, sizeof(own), 0))
  {
    memset(own, 0, sizeof(own));
    rec_add(fpath, own, sizeof(STOCK));
  }

  /* ç›®å‰è‚¡å¸‚ */
  if (rec_get(FN_RUN_STOCK, &sto, sizeof(STOCK), 0))
  {
    for (i = 0; i < MAXSTOCK; i++)
    {
      sto.price[i] = 100;
      sto.start[i] = 100;
      sto.change[i] = 0;
      sto.stillnum[i] = 100000;
      sto.ordernum[i] = 0;

    }
    rec_add(FN_RUN_STOCK, &sto, sizeof(STOCK));
  }

  while (1)
  {
    clear();
    outs("\033[1;36;44m â˜… â˜… â˜… â˜…  \033[31mã€ \033[35mè­‰åˆ¸äº¤æ˜“æ‰€\033[31m ã€‘"
      "  \033[36mâ˜… â˜… â˜… â˜…         [\033[32mDesign by Efen\033[36m] \033[m");

    show_stock_data(&sto);

    switch (vans("[B:è²· S:è³£ C:æŸ¥è©¢ F:æ¼²è·Œç´€éŒ„ I:èµ°å‹¢åœ– Q:é›¢é–‹]"))
    {
    case 'c':
      stock_query(&sto);
      break;

    case 'f':
      more(FN_RUN_STOCK_RECORD, NULL);
      break;

    case 'i':
      more(FN_RUN_STOCK_INFO, NULL);
      break;

    case 's':
      stock_sell(&sto);
      break;

    case 'b':
      stock_buy(&sto);
      break;


    case 'q':
      return 0;
    }
  }
}
#endif    /* HAVE_GAME */

